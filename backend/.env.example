# Environment
NODE_ENV=development

# Server
PORT=3000
FRONTEND_URL=http://localhost:5173

# Supabase Development
SUPABASE_URL_DEV=your_dev_supabase_url
SUPABASE_SERVICE_KEY_DEV=your_dev_supabase_service_key

# Supabase Production
SUPABASE_URL_PROD=your_prod_supabase_url
SUPABASE_SERVICE_KEY_PROD=your_prod_supabase_service_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-0125-preview  # or gpt-4, gpt-3.5-turbo
OPENAI_FT_MODEL=your_fine_tuned_model_name

# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=openai/gpt-4-0125-preview  # or anthropic/claude-3-opus-20240229

# YouTube API Configuration
YOUTUBE_CLIENT_ID=your_youtube_client_id
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_voice_id

# Pexels Configuration
PEXELS_API_KEY=your_pexels_api_key

# Reddit API Configuration
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=your_reddit_user_agent

# Replicate Configuration
REPLICATE_API_KEY=your_replicate_api_key

# AWS Configuration
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_INSTANCE_TYPE=t2.micro
AWS_AMI_ID=your_ami_id
AWS_KEY_NAME=your_key_name
AWS_SECURITY_GROUP=your_security_group
AWS_BASE_INSTANCE_NAME=your_instance_name 